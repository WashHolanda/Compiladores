/* Um programa para calcular a sequencia de fibonacci.*/

int c;

int fibonacci (int n){
    int next;
    int first;
    int second;
    c = 0;

    while(c<=n){
        if(c<=1){
            next = c;
        }
        else{
            next = first + second;
            first = second;
            second = next;
        }
        c = c + 1;
    }
    return next;
}

void main (void){
    int n;
    n = input();

    output(fibonacci(n));
}