/* Um programa para calcular a sequencia de fibonacci.*/

int cont;

int fibonacci (int n){
    int next;
    int first;
    int second;
    cont = 0;

    while(cont<=n){
        if(cont<=1){
            next = cont;
        }
        else{
            next = first + second;
            first = second;
            second = next;
        }
        cont = cont + 1;
    }
    return next;
}

void main (void){
    int n;
    n = input();

    output(fibonacci(n));
}